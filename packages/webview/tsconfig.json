{
  "compilerOptions": {
    "esModuleInterop": true,
    "jsx": "react",
    "lib": ["ES2021", "DOM", "DOM.Iterable"],
    "module": "esnext",
    "moduleResolution": "node",
    "noEmit": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitOverride": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strict": true,
    "target": "ES2021",
    "types": [
      "api",
      "react",
      "react-dom",
      "vscode-webview",
      "wicg-mediasession"
    ]
  },
  "include": ["src"]
}
