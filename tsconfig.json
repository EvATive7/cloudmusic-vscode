{
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "lib": ["es2019"],
    "module": "commonjs",
    "noEmit": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noUnusedParameters": true,
    "strict": true,
    "strictNullChecks": true,
    "target": "es2019"
  },
  "exclude": ["node_modules", ".vscode-test"],
  "include": ["src"]
}
